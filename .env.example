POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n

N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret
N8N_DEFAULT_BINARY_DATA_MODE=filesystem

# N8N API Key for workflow export automation
# Create in N8N: Settings → API → Create an API key
N8N_API_KEY=your-n8n-api-key

# For Mac users running OLLAMA locally
# See https://github.com/n8n-io/self-hosted-ai-starter-kit?tab=readme-ov-file#for-mac--apple-silicon-users
# OLLAMA_HOST=http://host.docker.internal:11434

# NGINX Proxy - External Services Configuration
# ==============================================
# URL du service RAG (webhook via ngrok ou autre)
# Exemple: https://abcd1234.ngrok.io
RAG_UPSTREAM_URL=https://your-ngrok-url.ngrok.io

# Braintrust API
# Clé API pour l'authentification
BRAINTRUST_API_KEY=sk-your-braintrust-api-key

# SupaBase Configuration
# URL de votre instance SupaBase
SUPABASE_URL=https://your-project.supabase.co
# Clé API SupaBase (anon ou service_role)
SUPABASE_API_KEY=sb_publishable_your-supabase-api-key

# Google Analytics Configuration
# Measurement Protocol (GA4) - pour envoyer des événements
GOOGLE_ANALYTICS_MEASUREMENT_ID=G-XXXXXXXXX
GOOGLE_ANALYTICS_API_SECRET=your-api-secret
# Token OAuth2 pour l'accès aux API Google Analytics Data et Admin (optionnel)
GOOGLE_ANALYTICS_TOKEN=your-google-oauth2-token
